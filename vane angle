import math
for i in range(11):
    R1=float(input('enter the R1:'))
    R2=float(input('enter the R2 :'))
    b1=float(input('enter the b1 angle :'))
    b2=float(input('enter the b2 angle :'))
    x=math.radians(b1)
    y=math.radians(b2)
    db1=math.cos(x)
    db2=math.cos(y)
    result=0
    numerator=(R2*R2)-(R1*R1)
    denomerator=2*((R2*db2)-(R1*db1))
    print('the answer is :',numerator/denomerator)
